					"script": {
						"exec": [
							"pm.test(\"Status code is 400\", function () {\r",
							"    pm.response.to.have.status(400);\r",
							"});\r",
							"\r",
							"pm.test(\"Nome único erro\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    if (responseJson.error === 'Uma ONG com esse nome já existe.') {\r",
